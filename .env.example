# Gemini API Key for DroidRun (Gemini 1.5/2.5 Flash)
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# DroidRun Configuration
DROIDRUN_MODEL=gemini-2.5-flash

# MobileRun Configuration (Cloud Fleet)
# Set to True to use Cloud. Set to False to use Local Android Device.
USE_MOBILE_RUN=False
MOBILERUN_API_KEY=your_mobilerun_api_key_here

